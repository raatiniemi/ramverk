<configurations parent="%directory.core.config%/autoload.xml">
	<configuration>
		<autoloads namespace="Me\Raatiniemi\Ramverk\Trunk">
			<autoload name="Action">%directory.application%/library/action/Action.class.php</autoload>
			<autoload name="View">%directory.application%/library/view/View.class.php</autoload>
		</autoloads>
		<autoloads namespace="Me\Raatiniemi\Ramverk\Trunk\Error">
			<autoload name="Action\NotFound">%directory.application.module%/Error/action/NotFound.class.php</autoload>
		</autoloads>
		<autoloads namespace="Me\Raatiniemi\Ramverk\Trunk\User">
			<autoload name="Action\Index">%directory.application.module%/User/action/Index.class.php</autoload>
			<autoload name="Action\Login">%directory.application.module%/User/action/Login.class.php</autoload>
			<autoload name="Action\Logout">%directory.application.module%/User/action/Logout.class.php</autoload>
			<autoload name="Action\Profile">%directory.application.module%/User/action/Profile.class.php</autoload>

			<autoload name="View\LoginFailure">%directory.application.module%/User/view/LoginFailure.class.php</autoload>
			<autoload name="View\LoginForm">%directory.application.module%/User/view/LoginForm.class.php</autoload>
			<autoload name="View\LoginSuccess">%directory.application.module%/User/view/LoginSuccess.class.php</autoload>
			<autoload name="View\LogoutSuccess">%directory.application.module%/User/view/LogoutSuccess.class.php</autoload>
			<autoload name="View\ProfileSuccess">%directory.application.module%/User/view/ProfileSuccess.class.php</autoload>
		</autoloads>
		<autoloads namespace="Me\Raatiniemi\Ramverk\Trunk\Index">
			<autoload name="Action\Index">%directory.application.module%/Index/action/Index.class.php</autoload>
			<autoload name="View\IndexSuccess">%directory.application.module%/Index/view/IndexSuccess.class.php</autoload>
		</autoloads>
	</configuration>
</configurations>